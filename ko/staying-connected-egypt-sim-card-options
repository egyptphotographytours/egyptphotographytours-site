        // âœ… í•œêµ­ì–´ ì‚¬ìš©ì ê²½í—˜ í–¥ìƒ
        function enhanceKoreanUserExperience() {
            // í•œêµ­ì–´ í˜ì´ì§€ì—ì„œë§Œ ì‹¤í–‰
            if (!window.location.pathname.includes('/ko/')) return;
            
            // í•œêµ­ì–´ ê´€ë ¨ ê²€ìƒ‰ì–´ ê°•ì¡°
            const koreanSearchTerms = [
                'ì´ì§‘íŠ¸ ìœ ì‹¬ ì¹´ë“œ êµ¬ë§¤',
                'ì¹´ì´ë¡œ ê³µí•­ì—ì„œ SIM ì¹´ë“œ ì‚¬ê¸°',
                'í•œêµ­ì¸ ì´ì§‘íŠ¸ ì—¬í–‰ ì¸í„°ë„·',
                'ì´ì§‘íŠ¸ì—ì„œ ì¹´ì¹´ì˜¤í†¡ ì‚¬ìš©í•˜ê¸°',
                'ì´ì§‘íŠ¸ VPN ì„¤ì • ë°©ë²•'
            ];
            
            // í•œêµ­ì–´ ì‚¬ìš©ìë¥¼ ìœ„í•œ ì¶”ê°€ ì •ë³´ í‘œì‹œ
            const koreanInfoSection = document.createElement('div');
            koreanInfoSection.className = 'korean-tip';
            koreanInfoSection.innerHTML = `
                <h4>ğŸ‡°ğŸ‡· í•œêµ­ì¸ ê´€ê´‘ê° ì‹¤ì‹œê°„ ì •ë³´</h4>
                <p><strong>ìµœì‹  ì—…ë°ì´íŠ¸:</strong> 2025ë…„ 3ì›” ê¸°ì¤€, Orange Egyptê°€ í•œêµ­ì¸ ê´€ê´‘ê°ì—ê²Œ ê°€ì¥ ì¶”ì²œë˜ëŠ” í†µì‹ ì‚¬ì…ë‹ˆë‹¤. ì¹´ì´ë¡œ ê³µí•­ ë„ì°© í™€ì—ì„œ ì˜ì–´ë¡œ ë„ì›€ì„ ë°›ìœ¼ì‹¤ ìˆ˜ ìˆìœ¼ë©°, 10-15GB ë°ì´í„° íŒ¨í‚¤ì§€ëŠ” ì•½ $15-20 ì •ë„ì…ë‹ˆë‹¤.</p>
                <p><strong>í•œêµ­ì–´ ì•± ì‘ë™:</strong> ì¹´ì¹´ì˜¤í†¡, ë„¤ì´ë²„, ì¹´ì¹´ì˜¤ë§µ ë“± ëŒ€ë¶€ë¶„ì˜ í•œêµ­ì–´ ì•±ì€ VPN ì—†ì´ë„ ì •ìƒ ì‘ë™í•©ë‹ˆë‹¤.</p>
            `;
            
            // ë‚´ìš©ì˜ ì ì ˆí•œ ìœ„ì¹˜ì— ì¶”ê°€
            const firstHighlightBox = document.querySelector('.highlight-box');
            if (firstHighlightBox) {
                firstHighlightBox.parentNode.insertBefore(koreanInfoSection, firstHighlightBox.nextSibling);
            }
            
            // í•œêµ­ì–´ ê´€ë ¨ ë§í¬ì— ì¶”ì  ì¶”ê°€
            document.querySelectorAll('a[href*="ko/"]').forEach(link => {
                link.addEventListener('click', function() {
                    if (typeof gtag !== 'undefined') {
                        gtag('event', 'korean_navigation', {
                            'event_category': 'Korean User',
                            'event_label': this.href
                        });
                    }
                });
            });
        }

        // âœ… í•œêµ­ì–´ ê²€ìƒ‰ ìµœì í™”ë¥¼ ìœ„í•œ ì¶”ê°€ ë©”íƒ€ íƒœê·¸
