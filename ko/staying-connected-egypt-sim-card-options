        function trackKoreanUserEngagement() {
            // í•œêµ­ì–´ í˜ì´ì§€ ì ‘ì† í†µê³„
            if (window.location.pathname.includes('/ko/')) {
                console.log('Korean user accessed SIM card guide');
                
                // í•œêµ­ì–´ ê´€ë ¨ ìš”ì†Œ í´ë¦­ íŠ¸ë˜í‚¹
                document.querySelectorAll('.korean-highlight, .korean-tip, .cta-button').forEach(element => {
                    element.addEventListener('click', function() {
                        console.log('Korean user interacted with: ', this.textContent.substring(0, 50));
                    });
                });
                
                // í•œêµ­ì–´ FAQ í™•ì¥ ì¶”ì 
                document.querySelectorAll('.faq-question').forEach(question => {
                    question.addEventListener('click', function() {
                        if (this.textContent.includes('í•œêµ­') || this.textContent.includes('í•œêµ­ì¸')) {
                            console.log('Korean user expanded Korean-specific FAQ');
                        }
                    });
                });
            }
        }

